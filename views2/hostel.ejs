<html>

<head>
    <title> HAB </title>
    <link href="https://unpkg.com/tailwindcss@^1.0/dist/tailwind.min.css" rel="stylesheet">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Spartan:300&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>

    <header class="text-gray-600 body-font md:px-32" style="background: #254D56; height: 110px;">
        <div class="container mx-auto flex flex-wrap px-4 py-5 flex-col md:flex-row items-center">
            <a class="flex title-font font-medium items-center text-gray-900 mb-4 md:mb-0">

                <img src="https://www.iitg.ac.in/stud/gymkhana/hostelBoard/images/logo.jpg" alt="hab-logo" class="h-10 rounded-full">
                <span class="ml-3 text-xl font-black text-3xl text-white">HAB</span>
            </a>
            <nav class="md:ml-auto flex flex-wrap items-center text-base justify-center">
                <a class="mr-5 hover:text-opacity-50 font-bold text-sm text-white" href="#notices">NOTICES</a>
                <a class="mr-5 hover:text-opacity-50 font-bold text-sm text-white" href="#">HOSTELS</a>
                <a class="mr-5 hover:text-opacity-50 font-bold text-sm text-white" href="#">MANAGEMENT</a>
                <a class="mr-5 hover:text-opacity-50 font-bold text-sm text-white" href="#">ORDINANCE AND RULES</a>
            </nav>
        </div>
    </header>


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <div id="carouselExampleIndicators" class="carousel slide carousel-fade" data-ride="carousel">

        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="6"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="7"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="8"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="9"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="10"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="11"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="12"></li>


        </ol>

        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="w-100 h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_barak.jpg" alt="First slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Barak Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100 h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_brahmaputra.jpg" alt="Second slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Brahmaputra Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/dhansiri.jpg" alt="third slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Dhansiri Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class=" w-100 h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_dibang.jpg" alt="fourth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Dibang Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_dihing.jpg" alt="fifth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Dihing Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/disang.jpg" alt="Sixth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Disang Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_kameng.jpg" alt="Seventh slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Kameng Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_kapili.jpg" alt="eighth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Kapili Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_manas.jpg" alt="nineth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Manas Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_siang.jpg" alt="tenth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Siang Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_subansiri.jpg" alt="eleventh slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Subansiri Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">

                <img class="w-100  h-50 d-inline-block" src="http://iitg.ac.in/freshers/res/images/small_umiam.jpg" alt="twelveth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Umiam Hostel</h5>
                </div>
            </div>
            <div class="carousel-item">
                <img class="w-100  h-50 d-inline-block" src="data:image/jpeg;base64,"
                    alt="Thirteenth slide">
                <div class="carousel-caption d-none d-md-block">
                    <h5>Lohit Hostel</h5>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <div class="flex justify-between">
        <a href="<%= link %>"><button  class="bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded mx-10">Add hostel</button></a>
        
    </div>

    <section style=" background: #0D1B1E;">
        <h2 style=" padding-left: 160px; padding-top: 75px; padding-bottom: 35px; color: white; padding-top: 95px; ">HOSTELS</h2>
        <br>


        <section class="text-gray-600 body-font " style="padding-left: 160px; padding-right: 160px; background: #0D1B1E; ">

            <div class="flex flex-wrap -m-4 ">
        <% hostels.forEach((hostel)=>{ %>
                <div class="lg:w-1/3 sm:w-1/2 p-4 ">
                    <div class="flex relative ">
                        <img alt=" gallery " class="absolute inset-0 w-full h-full object-cover object-center " src="../uploads/hostel/<%= hostel.pic %>">
                        <div class="carousel-caption h-0 ">
                            <h5><%=hostel.name%> Hostel</h5>
                        </div>
                        <div class="px-8 py-10 relative z-10 w-full border-4 border-gray-200 bg-white opacity-0 hover:opacity-100 ">

                            <h1 class=" title-font text-lg font-medium text-gray-900 mb-3 "><a href="hostels/<%=hostel.name%>"><%=hostel.name%> Hostel</a></h1>
                            <p class="leading-relaxed ">Photo booth fam kinfolk cold-pressed sriracha leggings jianbing microdosing tousled waistcoat.</p>
                        </div>
                        
                    </div>
                    <a class="inline-flex items-center">
                        <span class="justify-between flex flex-col pl-3">
                            <form class="mx-2" action="/hostels/<%=hostel.id%>?_method=DELETE" method="POST"><button  class="bg-red-500 hover:bg-red-600 text-white cursor-pointer  py-2 px-4 rounded">Delete</button></form>
                            <a href="/hostels/<%=hostel.name%>/updateHostel"><button  class="bg-red-500 hover:bg-red-600 text-white cursor-pointer  py-2 px-4 rounded">EDIT</button></a>
                        </span>
                      </a>
                </div>
        <% }) %>
            </div>
        </section>
</body>
<script src="https://cdn.jsdelivr.net/npm/tailwind@4.0.0/dist/tailwind.min.js "></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.6.347/pdf.min.js " integrity="sha512-Z8CqofpIcnJN80feS2uccz+pXWgZzeKxDsDNMD/dJ6997/LSRY+W4NmEt9acwR+Gt9OHN0kkI1CTianCwoqcjQ==" crossorigin=" anonymous "></script>

</html>

</html>